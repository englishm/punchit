<!DOCTYPE html>
%html
  %head
    %meta(charset="utf-8")
    %meta(content="IE=edge,chrome=1" http-equiv="X-UA-Compatible")
    %meta(name="viewport" content="width=device-width, initial-scale=1.0")
    %title= data.page.title || "The Middleman" 
    %link(href="/img/favicon.ico" rel="icon" type="image/x-icon")
    
    = stylesheet_link_tag "bootstrap.min" 
    = stylesheet_link_tag "bootstrap-responsive.min"
    = stylesheet_link_tag "select2"
    = stylesheet_link_tag "datepicker"
    = stylesheet_link_tag "jquery-ui-1.9.1.custom"
    = stylesheet_link_tag "all"

    = javascript_include_tag  "jquery-1.8.2.min"
    = javascript_include_tag  "jquery-ui-1.9.1.custom"

    = javascript_include_tag  "underscore-min"
    = javascript_include_tag  "backbone-min"
    = javascript_include_tag  "select2"
    = javascript_include_tag  "date"
    = javascript_include_tag  "bootstrap-datepicker"
    = javascript_include_tag  "jstorage"

    = javascript_include_tag  "bootstrap.min"
    = javascript_include_tag  "namespace"
    = javascript_include_tag  "d3.min"
    = javascript_include_tag  "bullet"
    = javascript_include_tag  "all"
 
  %body{class: page_classes}= yield
